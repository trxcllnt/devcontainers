{
  "ubuntu18.04": {
    "image": "ubuntu:18.04",
    "remoteUser": "coder",
    "containerEnv": {
      "gh_token": "${localEnv:gh_token}",
      "vault_host": "${localEnv:vault_host}",
      "VAULT_S3_TTL": "${localEnv:VAULT_S3_TTL}",
      "sccache_bucket_gh": "${localEnv:sccache_bucket_gh}",
      "sccache_region_gh": "${localEnv:sccache_region_gh}",
      "sccache_bucket_ci": "${localEnv:sccache_bucket_ci}",
      "sccache_region_ci": "${localEnv:sccache_region_ci}",
      "aws_access_key_id": "${localEnv:aws_access_key_id}",
      "aws_session_token": "${localEnv:aws_session_token}",
      "aws_secret_access_key": "${localEnv:aws_secret_access_key}"
    },
    "features": {
      "ghcr.io/devcontainers/features/common-utils:2": {
        "uid": "1000",
        "gid": "1000",
        "username": "coder",
        "installZsh": "true",
        "upgradePackages": "true"
      },
      "ghcr.io/devcontainers/features/git:1": {
        "ppa": "true"
      },
      "ghcr.io/devcontainers/features/git-lfs:1": {},
      "ghcr.io/devcontainers/features/github-cli:1": {},
      "gitlab-cli": {},
      "cmake": {},
      "ninja": {},
      "sccache": {
        "version": "0.5.0"
      },
      "utils": {}
    },
    "overrideFeatureInstallOrder": [
      "ghcr.io/devcontainers/features/common-utils",
      "ghcr.io/devcontainers/features/git",
      "ghcr.io/devcontainers/features/git-lfs",
      "ghcr.io/devcontainers/features/github-cli",
      "./gitlab-cli",
      "./cmake",
      "./ninja",
      "./sccache",
      "./utils"
    ]
  },
